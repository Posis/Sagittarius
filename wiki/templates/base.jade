- var defaultPageTitle = 'Sagittarius Wiki'
- var favicon = 'data:image/x-icon;base64,'

doctype html
html
	head
		block head
			title= pageTitle ? pageTitle : defaultPageTitle
			meta(charset='utf-8')
			meta(name='viewport', content='width=device-width, initial-scale=1')
			meta(name='author', content='William Gaul')
			link(rel='icon', type='image/x-icon', href=favicon)
			link(rel='stylesheet', type='text/css', href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.2/css/bootstrap.min.css')
			link(rel='stylesheet', type='text/css', href='https://cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/styles/tomorrow.min.css')
	body
		div.navbar.navbar-inverse.navbar-fixed-top
			div.container
				a.brand(href='/Sagittarius')= defaultPageTitle



		block content
		block footer
			footer.footer
				- var year = new Date().getFullYear()
				| Copyright &copy; #{year} 
				a(href=owner.uri)= owner.name
			script(type='text/javascript', src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
			script(type='text/javascript', src='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.2/js/bootstrap.min.js')
